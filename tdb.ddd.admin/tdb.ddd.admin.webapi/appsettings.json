{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Server": {
        "Url": "http://*:31030" //本服务url
      }
    }
  },
  //服务信息
  "Server": {
    "Seq": 0 //服务序号（0-127）【负载均衡场景】
  },
  //consul配置
  "Consul": {
    "IP": "127.0.0.1",
    "Port": 8500
  },
  //口令
  "Token": {
    "Issuer": "tdb.ddd", //发行者
    "SecretKey": "0123456789223456" //秘钥
  },
  //HashID
  "HashID": {
    "Salt": "tangdabinok"
  },
  "WhiteListIP": [ "127.0.0.1", "localhost", "::ffff:127.0.0.1" ], //白名单IP
  //cap
  "CAP": {
    "RedisConnStr": "127.0.0.1,defaultDatabase=0,password=", //redis链接字符串(StackExchange.Redis)
    "DBConnStr": "server=127.0.0.1;database=tdb.ddd.cap;user id=root;password=abc123456;Pooling=True;Allow User Variables=True;" //数据库链接字符串(mysql)
  }
}
