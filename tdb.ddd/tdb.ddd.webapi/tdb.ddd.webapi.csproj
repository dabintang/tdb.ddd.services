<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <OutputType>Library</OutputType>
  </PropertyGroup>

  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>tdb.ddd.webapi</Title>
    <Version>7.0.0.17</Version>
    <Authors>tangdabin</Authors>
    <Company>tdb</Company>
    <Product>tdb.ddd</Product>
    <Description>ddd webapi</Description>
    <PackageTags>tdb.ddd</PackageTags>
    <PackageReleaseNotes>ddd webapi</PackageReleaseNotes>
    <IsPackable>true</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="7.0.9" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Versioning.ApiExplorer" Version="5.1.0" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.5.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\tdb.ddd.application.contracts\tdb.ddd.application.contracts.csproj" />
    <ProjectReference Include="..\tdb.ddd.domain\tdb.ddd.domain.csproj" />
  </ItemGroup>
</Project>
