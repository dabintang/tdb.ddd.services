    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>tdb.ddd.repository.sqlsugar</Title>
    <Version>7.0.0.7</Version>
    <Authors>tangdabin</Authors>
    <Company>tdb</Company>
    <Product>tdb.ddd</Product>
    <Description>ddd sqlsugar</Description>
